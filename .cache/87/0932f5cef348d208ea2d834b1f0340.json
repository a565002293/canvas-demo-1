mouseup = () => {\r\n        painting = false\r\n    }\r\n\r\n}\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"cdd643a2ba1e18caf046a0ed546a7851","cacheData":{"env":{}}}